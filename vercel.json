{
  "version": 2,
  "name": "periodic-table-app",
  "installCommand": "npm install --legacy-peer-deps",
  "buildCommand": "npm run build",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/mocks/(.*)",
      "dest": "/public/api/mocks/$1",
      "headers": {
        "Cache-Control": "s-maxage=86400"
      }
    }
  ]
}